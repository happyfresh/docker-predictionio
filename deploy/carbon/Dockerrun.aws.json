{
  "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
    {
      "name": "predictionio",
      "image": "kahirul/predictionio:latest",
      "essential": true,
      "memory": 1536,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 7070
        },
        {
          "hostPort": 6066,
          "containerPort": 6066
        },
        {
          "hostPort": 7077,
          "containerPort": 7077
        },
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    }
  ]
}
